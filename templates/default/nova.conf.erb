<%= node["openstack"]["compute"]["custom_template_banner"] %>
<% @service_config.each do |section, values| -%>
[<%= section %>]
  <% values.each do |key, value| -%>
    <% if value.class == Hash -%>
<%= "# #{value['comment']}" -%>
<%= key %> = <%= value['set_to'] %>
    <% elsif value.class == Array -%>
      <% value.each do |list_value| %>
<%= key %> = <%= list_value %>
      <% end -%>
    <% else -%>
<%= key %> = <%= value %>
    <% end -%>
  <% end -%>
<% end -%>
